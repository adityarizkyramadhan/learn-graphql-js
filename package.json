{
  "scripts": {
    "createdb": "npx sequelize-cli db:create",
    "deletedb": "npx sequelize-cli db:drop",
    "migratedb": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migratedb",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "generate:seed": "npx sequelize-cli seed:generate --name ",
    "seed": "npx sequelize-cli db:seed:all",
    "start": "node app.js"
  },
  "dependencies": {
    "express": "^4.21.1",
    "graphql": "^16.9.0",
    "graphql-http": "^1.22.2",
    "mysql2": "^3.11.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  }
}
